{
  "name": "lunar-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for Lunar Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "echo \"Linting not configured\"",
    "build": "echo \"No build step required\"",
    "docker:build": "docker build -t lunar-backend .",
    "docker:run": "docker run -p 5000:5000 lunar-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": ["express", "api", "dashboard", "mock"],
  "author": "Lunar Team",
  "license": "MIT"
}

